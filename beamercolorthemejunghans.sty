\ProvidesPackage{beamercolorthemejunghans}[2014/03/01]

\makeatletter

\definecolor{jug@blau}{cmyk}{1, 0.7, 0, 0}
\definecolor{jug@grau}{cmyk}{0, 0, 0, 0.6}
\definecolor{jug@orange}{cmyk}{0, 0.3, 1, 0}

\setbeamercolor{structure}{fg=jug@blau}
\mode<presentation>
\setbeamercolor{palette primary}{fg=jug@blau,bg=white}
\setbeamercolor{palette secondary}{fg=jug@grau,bg=white}
\setbeamercolor{palette tertiary}{fg=black,bg=white}
\setbeamercolor{palette quaternary}{fg=jug@orange,bg=white}

\setbeamercolor{headline}{fg=white,bg=jug@grau}
\setbeamercolor{footline}{fg=white,bg=jug@grau}
\setbeamercolor{page number in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{author in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{title in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{date in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{institute in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{mini frame}{fg=orange,bg=jug@grau}
\setbeamercolor{section in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{subsection in head/foot}{fg=white,bg=jug@grau}
\setbeamercolor{subsubsection in head/foot}{fg=white,bg=jug@grau}

\mode<all>

\makeatother